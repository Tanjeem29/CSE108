<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SearchRegController">
   <children>
      <TextField fx:id="regID" layoutX="45.0" layoutY="46.0" prefHeight="25.0" prefWidth="209.0" promptText="Unique Registration Number" />
      <Button fx:id="searchButton" layoutX="274.0" layoutY="46.0" mnemonicParsing="false" onAction="#SearchReg" text="Search" />
      <Button fx:id="backButton" layoutX="516.0" layoutY="334.0" mnemonicParsing="false" onAction="#backToHome" text="Back" />
      <TextArea fx:id="results" editable="false" layoutX="50.0" layoutY="100.0" prefHeight="242.0" prefWidth="312.0" wrapText="true" />
   </children>
</AnchorPane>
